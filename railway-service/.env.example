# Railway Service Environment Variables
# Copy this to .env and fill with your actual values

# Supabase Configuration (Required)
SUPABASE_URL=https://ierdfxgeectqoekugyvb.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Upstash Redis Configuration (Required for QR & Pairing codes)
# Get these from https://console.upstash.com/redis
UPSTASH_REDIS_REST_URL=https://your-redis-instance.upstash.io
UPSTASH_REDIS_REST_TOKEN=your_redis_token_here

# Optional: Railway Static URL (auto-populated on Railway)
RAILWAY_STATIC_URL=

# Optional: Port (auto-populated on Railway)
PORT=3000

# Node Environment
NODE_ENV=production

# Logging Level (error, warn, info, debug)
LOG_LEVEL=info